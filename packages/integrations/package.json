{
  "name": "@rcuse/integrations",
  "version": "0.25.0",
  "description": "Integration wrappers for utility libraries",
  "author": "wangxingkang <https://github.com/wangxingkang>",
  "license": "MIT",
  "homepage": "https://github.com/rcuse/rcuse/tree/main/packages/integrations#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rcuse/rcuse.git",
    "directory": "packages/integrations"
  },
  "bugs": {
    "url": "https://github.com/rcuse/rcuse/issues"
  },
  "keywords": [
    "react",
    "react-use",
    "utils"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.cjs"
    },
    "./*": "./*",
    "./useFullscreen": {
      "import": "./useFullscreen.mjs",
      "require": "./useFullscreen.cjs"
    },
    "./useNProgress": {
      "import": "./useNProgress.mjs",
      "require": "./useNProgress.cjs"
    },
    "./useMqtt": {
      "import": "./useMqtt.mjs",
      "require": "./useMqtt.cjs"
    },
    "./useSupercluster": {
      "import": "./useSupercluster.mjs",
      "require": "./useSupercluster.cjs"
    },
    "./useScrollbarWidth": {
      "import": "./useScrollbarWidth.mjs",
      "require": "./useScrollbarWidth.cjs"
    },
    "./useCopyToClipboard": {
      "import": "./useCopyToClipboard.mjs",
      "require": "./useCopyToClipboard.cjs"
    }
  },
  "main": "./index.cjs",
  "module": "./index.mjs",
  "unpkg": "./index.iife.min.js",
  "jsdelivr": "./index.iife.min.js",
  "types": "./index.d.cts",
  "peerDependencies": {
    "@pansy/copy-to-clipboard": "*",
    "@pansy/scrollbar-width": "*",
    "mqtt": "^4",
    "nprogress": "*",
    "screenfull": "*",
    "supercluster": ">=8"
  },
  "peerDependenciesMeta": {
    "@pansy/copy-to-clipboard": {
      "optional": true
    },
    "@pansy/scrollbar-width": {
      "optional": true
    },
    "mqtt": {
      "optional": true
    },
    "nprogress": {
      "optional": true
    },
    "screenfull": {
      "optional": true
    },
    "supercluster": {
      "optional": true
    }
  },
  "dependencies": {
    "@rcuse/core": "workspace:*",
    "@types/geojson": "*"
  },
  "devDependencies": {
    "@pansy/copy-to-clipboard": "catalog:",
    "@pansy/scrollbar-width": "catalog:",
    "@types/nprogress": "catalog:",
    "@types/supercluster": "catalog:",
    "mqtt": "catalog:",
    "nprogress": "catalog:",
    "screenfull": "catalog:",
    "supercluster": "catalog:"
  }
}
